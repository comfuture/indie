{
  "name": "indie",
  "version": "0.1.0",
  "description": "An indexDB wrapper that follows contemporary es6 styles.",
  "main": "/lib/indie.js",
  "scripts": {
    "debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "dev": "webpack --progress --colors --watch --env dev --mode=development --module-bind js=babel-loader ",
    "build": "webpack --env dev --mode=development --module-bind js=babel-loader && npm run test ",
    "repl": "local-repl"
  },
  "keywords": [
    "js",
    "library",
    "indexdb",
    "es6"
  ],
  "author": "Changkyun Kim",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-async-generators": "^6.13.0",
    "babel-preset-env": "^1.6.1",
    "doctest": "^0.14.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "is-plain-object": "^2.0.4",
    "jest": "^22.4.3",
    "local-repl": "^3.1.0",
    "prettier-standard": "^8.0.1",
    "regenerator-runtime": "^0.11.1",
    "shelving-mock-indexeddb": "^1.0.7",
    "standard": "^11.0.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "yargs": "^11.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
